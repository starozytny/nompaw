{% extends 'user/pages/profil/template.html.twig' %}

{% set page_title %}{{ app.user.lastname }} {{ app.user.firstname }}{% endset %}
{% set page_subtitle %}Profil de {{ app.user.lastname }} {{ app.user.firstname }}{% endset %}

{% block breadcrumb %}
    {% set breadcrumbs = [{ text: page_title }] %}
    {{ parent() }}
{% endblock %}

{% block main_content %}
    {% include "layout/components/admin/page_toolbar.html.twig" with { page_back: path('admin_users_index', {'h': elem.id}) } %}

    <div id="users_update"
         data-obj="{{ obj }}"
    >
        {% include "layout/components/loader.html.twig" %}
    </div>
{% endblock %}
